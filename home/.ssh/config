Host asterope
	user amxx
	Hostname 2a01:cb14:1a1:7300:ba27:ebff:fe45:98e9
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_ed25519
Host asterope-remote
	user amxx
	Hostname 192.168.1.135
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_ed25519
	ProxyCommand ssh -W %h:%p maia-remote 2> /dev/null
Host asterope-in
	user amxx
	Hostname 192.168.1.135
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_ed25519

