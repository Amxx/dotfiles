Host ens ssh.ens-lyon.fr
	user hcrouboi
	Hostname ssh.ens-lyon.fr
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_ed25519

Host *.dsi-ext.ens-lyon.fr
	user hcrouboi
	Hostname %h
	HostkeyAlgorithms ssh-dss
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_ed25519
	ProxyCommand ssh -W %h:%p ens 2> /dev/null
Host slsu*
	user hcrouboi
	Hostname %h.dsi-ext.ens-lyon.fr
	HostkeyAlgorithms ssh-dss
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_ed25519
	ProxyCommand ssh -W %h:%p ens 2> /dev/null

